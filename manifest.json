{
  "manifest_version": 3,
  "name": "newfinal - Geo-Track Browser Extension",
  "version": "1.0",
  "description": "Tracks geolocation of servers and detects anomalies.",
  "permissions": [
    "webRequest",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>",
    "http://ip-api.com/*",
    "https://ipapi.co/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "browser_action": {},
  "web_accessible_resources": [
    {
      "resources": ["tf.min.js", "model-worker.js", "dataset.json"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_page": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      },
      "description": "Opens Geo-Track Monitor"
    }
  }
}